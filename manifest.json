{
  "manifest_version": 3,
  "name": "K-SUITE",
  "version": "1.0.0",
  "description": "K-LARC, K-Query, K-SCAN 통합 확장프로그램",
  "action": {
    "default_title": "K-SUITE",
    "default_popup": "suite/popup.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "activeTab",
    "debugger",
    "sidePanel",
    "clipboardWrite"
  ],
  "host_permissions": [
    "http://10.133.111.32:8080/*",
    "<all_urls>"
  ],
  "side_panel": {
    "default_path": "suite/sidepanel-home.html"
  },
  "icons": {
    "16": "modules/k-query/icons/icon-16.png",
    "32": "modules/k-query/icons/icon-32.png",
    "48": "modules/k-query/icons/icon-48.png",
    "128": "modules/k-query/icons/icon-128.png"
  }
}
